{
  "name": "website",
  "version": "1.0.0",
  "description": "Webpage for Unibeautifier",
  "private": true,
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs": "npm run clean && npm run generate-docs",
    "clean": "npm run rm-languages && npm run rm-beautifiers && npm run rm-options",
    "rm-beautifiers": "rimraf 'docs/beautifier-*'",
    "rm-languages": "rimraf 'docs/language-*'",
    "rm-options": "rimraf 'docs/option-*'",
    "generate-docs": "ts-node scripts/generate-docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/website.git"
  },
  "keywords": [],
