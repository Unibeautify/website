  "bugs": {
    "url": "https://github.com/Unibeautify/website/issues"
  },
  "homepage": "https://github.com/Unibeautify/website#readme",
  "devDependencies": {
    "@types/diff": "^3.2.2",
    "@types/fs-extra": "^5.0.0",
    "@types/lodash": "^4.14.92",
    "@unibeautify/beautifier-eslint": "^0.2.0",
    "@unibeautify/beautifier-js-beautify": "^0.3.0",
    "@unibeautify/beautifier-prettier": "^0.6.0",
    "@unibeautify/beautifier-prettydiff": "^0.5.0",
    "diff": "^3.4.0",
    "eslint": "^4.17.0",
